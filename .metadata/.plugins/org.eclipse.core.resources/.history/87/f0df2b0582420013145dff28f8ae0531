package ch.fhnw.error404.DerGrosseDalmuti.shared;

public class Role {

	private boolean multiUse;
	private int code;
	public enum ROLE_TYPE{
		GROSSERDALMUTI(1, "grosser Dalmuti"), 
		KLEINERDALMUTI(2, "kleiner Dalmuti"), 
		BUERGER(3, "Bürger"), 
		KLEINERDIENER(4, "kleiner Diener"), 
		GROSSERDIENER(5, "grosser Diener");
		
		private final int code;
		private final String label;
		
		// Konstruktor mit Parameteruebergabe:
		ROLE_TYPE(int code, String label){
			this.code=code;
			this.label=label;
		}
		
		String getLabel(){return label;}
		int getCode(){return code;}
	};	
	

	
	Role(boolean multiUse, int code){
		
	}

	
	
}
